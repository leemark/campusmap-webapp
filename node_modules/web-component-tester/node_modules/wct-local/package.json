  },
  "main": "lib/plugin.js",
  "scripts": {
    "postinstall": "node scripts/postinstall.js"
  },
  "wct-plugin": {
    "cli-options": {
      "browsers": {
        "help": "Local browsers to run tests on, or 'all'.",
        "full": "local",
        "metavar": "NAME",
        "abbr": "l",
        "list": true
